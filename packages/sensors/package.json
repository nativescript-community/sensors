{
    "name": "@nativescript-community/sensors",
    "version": "1.3.9",
    "description": "Nativescript plugin for Carto Mobile SDK",
    "main": "index",
    "typings": "index.d.ts",
    "sideEffects": false,
    "nativescript": {
        "platforms": {
            "android": "4.2.0",
            "ios": "4.2.0"
        }
    },
    "scripts": {
        "build": "npm run tsc",
        "build.all": "npm run build",
        "readme": "readme generate -c ../../tools/readme/blueprint.json",
        "tsc": "cpy '**/*.d.ts' '../../packages/sensors' --parents --cwd=../../src/sensors && tsc -skipLibCheck -d",
        "clean": "rimraf ./*.d.ts ./*.js ./*.js.map"
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "iOS"
    ],
    "author": {
        "name": "Martin Guillon",
        "email": "martin.guillon@akylas.fr"
    },
    "bugs": {
        "url": "https://github.com/nativescript-community/sensors/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/nativescript-community/sensors",
    "bootstrapper": "nativescript-plugin-seed",
    "dependencies": {
        "@nativescript-community/perms": "^2.3.0"
    }
}
